library(ggplot2)
# Sample Data
set.seed(123)
df <- data.frame(x = 1:100, y = sin(1:100/10) + rnorm(100, 0, 0.2))
# Plot with LOESS smoothing
ggplot(df, aes(x, y)) +
geom_point() +
geom_smooth(method = "loess", span = 0.2, color = "blue") + # LOESS curve
theme_minimal()
